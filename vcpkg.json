{
    "name": "alia",
    "builtin-baseline": "66b4b34d99ab272fcf21f2bd12b616e371c6bb31",
    "features": {
        "html": {
            "description": "Build HTML integration",
            "dependencies": [
                "nlohmann-json",
                "cereal",
                "scnlib"
            ]
        },
        "ui": {
            "description": "Build the UI library",
            "dependencies": [
                "fmt",
                {
                    "name": "skia",
                    "default-features": false,
                    "features": [
                        "harfbuzz",
                        "icu",
                        "gl"
                    ]
                }
            ]
        },
        "glfw": {
            "description": "GLFW backend for UI library",
            "dependencies": [
                "glfw3"
            ]
        },
        "tests": {
            "description": "Build tests",
            "dependencies": [
                "catch2"
            ]
        }
    },
    "vcpkg-configuration": {
        "overlay-triplets": [
            "vcpkg/triplets/"
        ]
    }
}